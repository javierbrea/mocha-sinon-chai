{
  "name": "mocha-sinon-chai",
  "version": "1.0.0",
  "description": "Wrapper for Mocha, Sinon and Chai. Avoid globals in tests",
  "main": "index.js",
  "keywords": [
    "mocha",
    "sinon",
    "chai",
    "test",
    "testing",
    "unit test",
    "standardjs",
    "coverage"
  ],
  "scripts": {
    "coveralls": "cat ./.coverage/lcov.info | ./node_modules/.bin/coveralls",
    "standard": "standard",
    "test": "./node_modules/istanbul/lib/cli.js cover ./node_modules/.bin/_mocha -- --recursive test"
  },
  "dependencies": {
    "chai": "4.1.2",
    "dirty-chai": "2.0.1",
    "mocha": "4.0.1",
    "sinon": "4.1.2",
    "sinon-chai": "2.14.0"
  },
  "devDependencies": {
    "coveralls": "3.0.0",
    "istanbul": "0.4.5",
    "standard": "10.0.3"
  },
  "engines": {
    "node": "8.9.4",
    "npm": "5.6.0"
  },
  "author": {
    "name": "Javier Brea",
    "url": "http://javierbrea.com"
  },
  "license": "MIT",
  "repository": "https://github.com/javierbrea/mocha-sinon-chai"
}
